<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>de.skycoder42.xport</string>
	<key>Program</key>
  <string>xport</string>
	<key>ProgramArguments</key>
	<array>
		<string>--log-level</string>
		<string>ALL</string>
		<string>--project-dir</string>
		<string>...</string>
	</array>
	<key>ProcessType</key>
	<string>Background</string>
	<key>StandardErrorPath</key>
	<string>/var/log/xport_err.log</string>
	<key>StandardOutPath</key>
	<string>/var/log/xport.log</string>
  <key>StartCalendarInterval</key>
  <dict>
    <key>Weekday</key>
    <integer>1-5</integer>
    <key>Hour</key>
    <integer>10</integer>
    <key>Minute</key>
    <integer>30</integer>
  </dict>
</dict>
</plist>
